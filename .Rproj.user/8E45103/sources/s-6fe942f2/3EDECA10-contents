@article{some_key1,
	title = {A fake article's title where you are first author},
	doi = {DOI},
	journal = {JOURNAL},
	author = {Lastname, Firstname and Someone, Else and Another, guy},
	month = jun,
	year = {2018},
	first = TRUE
}


